{
  "name": "electron-quick-start-typescript",
  "version": "1.0.0",
  "description": "A minimal Electron application written with Typescript",
  "scripts": {
    "build": "webpack --config ./webpack.config.js",
    "start": "npm run build && electron ./dist/electron.js",
    "watch": "webpack --watch --config ./webpack.config.js && electron ./dist/electron.js",
    "lint": "tslint -c tslint.json -p tsconfig.json",
    "test": "mocha -r ts-node/register -r ignore-styles test/**/*.spec.ts"
  },
  "repository": "https://github.com/electron/electron-quick-start-typescript",
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo",
    "typescript"
  ],
  "author": "GitHub",
  "license": "CC0-1.0",
  "devDependencies": {
    "@types/chai": "^4.2.11",
    "@types/mocha": "^7.0.2",
    "@types/node": "^13.11.1",
    "@types/react": "^16.9.34",
    "@types/react-dom": "^16.9.6",
    "bootstrap": "^4.4.1",
    "chai": "^4.2.0",
    "css-loader": "^3.5.2",
    "electron": "^15.5.5",
    "html-webpack-plugin": "^4.2.0",
    "ignore-styles": "^5.0.1",
    "konva": "^4.2.2",
    "mocha": "^7.1.1",
    "node-sass": "^4.13.1",
    "react": "^16.13.1",
    "react-bootstrap": "^1.0.0",
    "react-dom": "^16.13.1",
    "react-konva": "^16.13.0-0",
    "redux": "^4.0.5",
    "sass": "^1.26.3",
    "sass-loader": "^8.0.2",
    "spectron": "^10.0.1",
    "style-loader": "^1.1.3",
    "ts-loader": "^6.2.2",
    "ts-node": "^8.8.2",
    "tslint": "^5.19.0",
    "typescript": "^3.6.0",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11",
    "webpack-electron-reload": "^1.0.1"
  }
}